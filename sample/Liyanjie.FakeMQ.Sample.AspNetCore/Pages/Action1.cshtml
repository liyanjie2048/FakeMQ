@page

@inject Liyanjie.FakeMQ.FakeMQEventBus EventBus

@{
    Layout = null;

    var message = $"Action1:{DateTimeOffset.Now.ToString("yyyyMMddHHmmssfffffffzzzz")}";
    await EventBus.PublishAsync(new Liyanjie.FakeMQ.Sample.AspNetCore.Domains.MessageEvent { Message = message });
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Action1</title>
</head>
<body>
    @message
</body>
</html>
