@page

@inject Liyanjie.FakeMQ.Sample.AspNetCore_3_0.Infrastructure.DataContext Context

@{
    Layout = null;
    var list = Context.Messages.ToList();
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @foreach (var item in list)
    {
        <p>@item.Id=>@item.Content</p>
    }
</body>
</html>
